run = "./main"
hidden = ["main"]
compile = "build.bash"

[languages.go]
pattern = "**/*.go"

[languages.go.languageServer]
start = "gopls"

[nix]
channel = "stable-22_11"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["sh", "-c", "./main"]
deploymentTarget = "cloudrun"
build = ["sh", "-c", "build.bash"]
